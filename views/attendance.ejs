<%- include("partials/header") %>
<h2 class="mt-4">Attendance History</h2>
<table class="table table-bordered mt-3">
  <thead>
    <tr>
      <th>Date</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% attendance.forEach(a => { %>
      <tr>
        <td><%= a.date.toISOString().split('T')[0] %></td>
        <td><%= a.status %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
<%- include("partials/footer") %>
